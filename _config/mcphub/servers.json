{
  "nativeMCPServers": {
    "neovim": {
      "disabled": true
    }
  },
  "mcpServers": {
    "markitdown": {
      "disabled": false,
      "args": [
        "run",
        "--rm",
        "-i",
        "markitdown-mcp:latest"
      ],
      "command": "podman"
    },
    "blender": {
      "name": "github.com/ahujasid/blender-mcp",
      "args": [
        "blender-mcp"
      ],
      "command": "uvx"
    },
    "tavily": {
      "disabled": true,
      "name": "tavily",
      "args": [
        "-y",
        "tavily-mcp@0.1.3"
      ],
      "command": "npx"
    },
    "fetch": {
      "disabled": true,
      "name": "github.com/modelcontextprotocol/servers/tree/main/src/fetch",
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx"
    },
    "git": {
      "name": "github.com/modelcontextprotocol/servers/tree/main/src/git",
      "args": [
        "mcp-server-git",
        "--repository",
        "/workspace/a-light.win/corner-vault"
      ],
      "command": "uvx"
    }
  }
}